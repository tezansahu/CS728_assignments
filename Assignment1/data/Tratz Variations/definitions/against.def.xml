<?xml version="1.0"?>

<defset prep="against">
	<sense id="1">
		<definition>indicating an opposed, mitigated, or disfavored party/thing, including conceptual abstractions (e.g., argued against it)</definition>
<!--in opposition to; indicating an opposing force or competitor-->
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" unionof="1(1),2(1a),3(1b),4(2),5(2a),6(2b)"/>
	</sense>
	<sense id="2">
		<definition>indicating opposition by physical forces, usually involving contact (e.g., the broom against the wall)</definition>
		<majorcluster>SPATIAL</majorcluster>
		<pprojmap type="equivalent" targetid="10(4)"/>
	</sense>
	<sense id="3">
		<definition>in relation to (an amount of money owed, due, or lent) so as to reduce, cancel, or secure it (e.g., paid money against his debts)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid="7(2c)"/>
	</sense>
	<sense id="4" dumby="true">
		<definition>in contrast to</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid=""/>
	</sense>
	<sense id="4.1">
		<definition>in conceptual contrast to (e.g., must compare it against the alternative)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid="8(3)"/>
	</sense>
	<sense id="4.2">
		<definition>in visual contrast to (e.g., the yellow stood out against the black background)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid="9(3a)"/>
	</sense>

<!--	<sense id="3">
		<definition>(in the context of odds or betting) indicating the party or outcome disfavored</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid=""/>
		<intmap type="equivalent" word="" targetid=""/>
	</sense>-->
	
</defset>
