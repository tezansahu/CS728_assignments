<?xml version="1.0"?>

<defset prep="for">
	<sense id="1">
		<definition>used to or intended to contain (e.g., jar for cookies)</definition>
		<majorcluster>PURPOSE</majorcluster>
		<pprojmap type="equivalent" subsetof="5(4)"/>
		<notes></notes>
	</sense>
	<sense id="2">
		<definition>with the purpose of mitigating, preventing, or reducing (e.g., cure for cancer)</definition>
		<majorcluster>PURPOSE</majorcluster>
		<pprojmap type="equivalent" subsetof="5(4)"/>
		<notes></notes>
	</sense>
	<sense id="3">
		<definition>with the purpose of performing or engaging in (an activity) (e.g., hammer for building)</definition>
		<majorcluster>PURPOSE</majorcluster>
		<pprojmap type="equivalent" subsetof="5(4)"/>

		<notes></notes>
	</sense>
	<sense id="4">
		<definition>with the purpose of obtaining, accessing, experiencing, or detecting (e.g., asked for the book, paid $5 for it)</definition>
		<majorcluster>PURPOSE</majorcluster>
		<pprojmap type="equivalent" subsetof="5(4)"/>
		<notes></notes>
	</sense>
	<sense id="5">
		<definition>indicating something given or required in exchange (e.g., bought it for $5)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid="10(8a)"/>
		<notes>somewhat ambiguous with with definition 4 as in 'sold it for money'</notes>
	</sense>
	<sense id="6">
		<definition>indicating the topic of desire (e.g., longed for peace)</definition>
		<majorcluster>TOPIC</majorcluster>
		<pprojmap type="equivalent" subsetof="6(5)"/>
		<notes>emotional cousin of definition 4</notes>
	</sense>

	<sense id="7">
		<definition>indicating the focus of an emotion (other than desire) (e.g., disdain for chocolate)</definition>
		<majorcluster>TOPIC</majorcluster>
		<pprojmap type="subsumes" subsetof="2(2)"/>
		<pprojmap type="subsumes" subsetof="3(3)"/>
		<notes></notes>
	</sense>
	
	<sense id="8">
		<definition>indicating the destination of movement (e.g., headed for the door)</definition>
		<majorcluster>DESTINATION</majorcluster>
		<pprojmap type="equivalent" targetid="7(6)"/>
		<notes></notes>
	</sense>
	<sense id="9">
		<definition>indicating the intended/expected recipient (e.g., bought it for her)</definition>
		<majorcluster>RECIPIENT</majorcluster>
		<pprojmap type="equivalent" subsetof="3(3)"/>
		<notes></notes>
	</sense>
	<sense id="10">
		<definition>benefiting; in favor of (e.g., ruled for the defense)</definition>
		<majorcluster>BENEFACTIVE</majorcluster>
		<pprojmap type="subsumes" subsetof="1(1)"/>
		<pprojmap type="subsumes" targetid="3(3)"/>

		<notes></notes>
	</sense>
	<sense id="11">
		<definition>indicating the employer (e.g., worked for the bank)</definition>
		<majorcluster>EMPLOYER</majorcluster>
		<pprojmap type="equivalent" targetid="4(3a)"/>
		<notes></notes>
	</sense>
	<sense id="12" dumby="true">
		<definition>indicating an extent</definition>
	</sense>
	<sense id="12.1">
		<!-- change to 'duration', so much clearer -->
		<definition>indicating the temporal extent (e.g., for 16 minutes)</definition>
		<majorcluster>TEMPORAL</majorcluster>
		<pprojmap type="equivalent" targetid="12(10)"/>
		<notes></notes>
	</sense>
	<sense id="12.2">
		<definition>indicating the distance/extent (e.g., for 7 miles)</definition>
		<majorcluster>EXTENT</majorcluster>
		<pprojmap type="equivalent" targetid="13(11)"/>
		<notes></notes>
	</sense>
	<sense id="13">
		<definition>indicating the ordinal position within a series (e.g., for the 5th time)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid="14(12)"/>
		<notes></notes>
	</sense>
	<sense id="14">
		<!-- NOTE: there may be some ambiguity with definition 7 -->
		<definition>indicating the justification (and, perhaps also the topic) (e.g., criticize for losing)</definition>
		<majorcluster>JUSTIFICATION</majorcluster>
		<pprojmap type="equivalent" subsetof="6(5)"/>
		<notes>Mourning/grieving for someone/something is treated as justification.</notes>
	</sense>
	<sense id="15">
		<definition>relative to the expected norm (e.g., soft for a turtle shell)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="equivalent" targetid="11(9)"/>
		<notes></notes>
	</sense>	
	<sense id="16">
		<definition>regarding; with respect to; in relation to; with purpose(s) related to (e.g., a name for it)</definition>
		<majorcluster></majorcluster>
		<pprojmap type="subsumes" targetid="2(2)"/>
		<pprojmap type="subsumes" subsetof="2(2)-1"/>
		<pprojmap type="subsumes" subsetof="5(4)"/>
		<notes></notes>
	</sense>
<!--<sense id="">
		<definition>on the part of (e.g., fun for them)</definition>
		<majorcluster>EXPERIENCER</majorcluster>
		<notes></notes>
	</sense>-->
<!--	<sense id="">
		<definition>some other purpose not captured by other definitions; often 'for use with/in-relation-to' (e.g., a mouse for his computer)</definition>
		<majorcluster>PURPOSE</majorcluster>
		<notes></notes>
	</sense>-->
	
</defset>
